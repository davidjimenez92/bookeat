@model IEnumerable<Proyecto_net.Models.Valoracion>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Valor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Comentario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdpedidoNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Valor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Comentario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdpedidoNavigation.Idpedido)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Idvaloracion">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Idvaloracion">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Idvaloracion">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
